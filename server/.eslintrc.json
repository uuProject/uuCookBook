{
  "env":{
    "browser":true,
    "es2021":true
  },
  "extends":[
    "airbnb-base"
  ],
  "parserOptions":{
    "ecmaVersion":"latest",
    "sourceType":"module"
  },
  "rules":{
    "indent":[
      "error",
      2
    ],
    "semi":[
      2,
      "always"
    ],
    "eol-last":2,
    "import/prefer-default-export": 0,
    "import/extensions": 0
  }
}
